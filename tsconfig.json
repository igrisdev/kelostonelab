{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@sections/*": ["src/sections/*"],
      "@libs/*": ["src/libs/*"],
      "@components_functional/*": ["src/components_functional/*"],
      "@assets/*": ["src/assets/*"],
      "@services/*": ["src/services/*"],
      "@types/*": ["src/types/*"],
      "@stores/*": ["src/stores/*"],
      "@layouts/*": ["src/layouts/*"],
      "@graphql/*": ["src/graphql/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
